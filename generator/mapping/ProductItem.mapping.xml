<?xml version="1.0" encoding="UTF-8"?>
<mapping>
    <entity name="ProductItem" package="com.glaf.crowd" table="T_PRODUCT_ITEM" moduleName="crowd"
	        title="产品项" englishTitle="ProductItem">
        <id name="id" column="ID_" type="String" title="编号" englishTitle="Id" length="64"/>
		<property name="tenantId" column="TENANTID_" type="String" title="租户编号" englishTitle="tenantId" length="50"
		          editable="false"/>
		<property name="category" column="CATEGORY_" type="String" title="分类" englishTitle="Category" 
		          editable="true" length="50"  searchable="true" displayType="4"/>
		<property name="itemName" column="ITEMNAME_" type="String" title="发起人" englishTitle="ItemName" 
		          editable="true" length="100"  searchable="true" displayType="4"/>
		<property name="itemLocation" column="ITEMLOCATION_" type="String" title="地址" englishTitle="ItemLocation" 
		          length="200" editable="true" searchable="true" displayType="4"/>
		<property name="smallUrl" column="SMALLURL_" type="String" title="小图片地址" englishTitle="SmallUrl" 
		          editable="true" length="250"  searchable="true" displayType="4"/>
		<property name="itemUrl" column="ITEMURL_" type="String" title="图片地址" englishTitle="ItemUrl" 
		          editable="true" length="250"  searchable="true"/>
		<property name="itemTitle" column="ITEMTITLE_" type="String" title="标题" englishTitle="ItemTitle" 
		          editable="true" length="250"  searchable="true" displayType="4"/>
		<property name="itemContent" column="ITEMCONTENT_" type="String" title="内容描述" englishTitle="ItemContent" 
		          editable="true" length="4000"  searchable="true"/>
		<property name="itemStatus" column="ITEMSTATUS_" type="Integer" title="进度" englishTitle="ItemStatus" 
		          editable="true" displayType="4"/>
		<property name="itemMoney" column="ITEMMONEY_" type="Double" title="金额" englishTitle="ItemMoney" 
		          editable="true" searchable="true" displayType="4"/>
        <property name="itemDay" column="ITEMDAY_" type="Integer" title="有效天数" englishTitle="ItemDay" editable="true" displayType="4"/>
		<property name="lastModified" column="LASTMODIFIED_" type="Long" title="最后修改时间戳" englishTitle="LastModified" 
		          editable="false"/>
		<property name="createBy" column="CREATEBY_" type="String" title="创建人" englishTitle="CreateBy" length="50" editable="false"/>
        <property name="createTime" column="CREATETIME_" type="Date" title="创建时间" englishTitle="CreateTime" editable="false"/>
        <property name="updateBy" column="UPDATEBY_" type="String" title="更新人" englishTitle="UpdateBy" length="50" editable="false"/>
        <property name="updateTime" column="UPDATETIME_" type="Date" title="更新时间" englishTitle="UpdateTime" editable="false"/>
    </entity>
</mapping>